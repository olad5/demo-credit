{
  "root": true,
  "parser": "@typescript-eslint/parser",
  "plugins": [
    "@typescript-eslint",
    "prettier"
  ],
  "extends": [
    "eslint:recommended",
    "plugin:@typescript-eslint/eslint-recommended",
    "plugin:@typescript-eslint/recommended",
    "prettier"
  ],
  "rules": { 
    "no-console": 2 ,
    "no-duplicate-imports": 2 ,
    "@typescript-eslint/no-empty-interface": 1,
    "prettier/prettier": 2
  }
}
